{
  "compilerOptions": {
    "esModuleInterop": true,
      "jsx": "react",
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*", "./dist/*", ""],
        "pages/*": ["src/pages/*"],
        "components/*": ["src/components/*"],
      //   "types/*": ["src/@types/*"],
        "public/*": ["public/*"],
        "img/*": ["src/assets/img/*"],
        "styles/*": ["src/styles/*"]
      }
